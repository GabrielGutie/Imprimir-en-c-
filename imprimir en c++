#include <iostream>
using namespace std;
int main() {
   cout << "Nombre: ";
   string nombre;
   getline(cin, nombre);
   cout << "Fecha de nacimiento: ";
   string fecha;
   getline(cin, fecha);
   cout << "Edad: ";
   string edad;
   getline(cin, edad);
   cout << "Hobby favorito: ";
   string hobby;
   getline(cin, hobby);
   cout << "Grupo/Artista favorito: ";
   string grupo;
   getline(cin, grupo);
}
